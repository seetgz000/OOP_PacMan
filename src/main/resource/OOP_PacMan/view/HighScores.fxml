<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="400.0" styleClass="background" stylesheets="@../style/Style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="OOP_PacMan.controller.UserController">
    <children>
        <SplitPane dividerPositions="0.8445" orientation="VERTICAL" prefHeight="600.0" prefWidth="400.0" styleClass="background" stylesheets="@../style/Style.css">
            <items>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                    <children>
                        <Label layoutX="80.0" layoutY="31.0" prefHeight="53.0" prefWidth="249.0" styleClass="font" text="HIGH SCORES" textFill="WHITE">
                            <font>
                                <Font size="27.0" />
                            </font>
                        </Label>
                        <TableView fx:id="itemtableview" layoutY="110.0" prefHeight="388.0" prefWidth="369.0" styleClass="table-view">
                            <columns>
                                <TableColumn fx:id="nameColumn" prefWidth="182.0" text="Name" />
                                <TableColumn fx:id="scoreColumn" prefWidth="186.0" text="Score" />
                            </columns>
                        </TableView>
                    </children>
                </AnchorPane>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                    <children>
                        <HBox layoutX="-34.0" layoutY="31.0" prefHeight="69.0" prefWidth="434.0">
                            <children>
                                <Button mnemonicParsing="false" onAction="#backtoMain" prefHeight="39.0" prefWidth="99.0" styleClass="font" text="BACK">
                                    <HBox.margin>
                                        <Insets left="55.0" top="10.0" />
                                    </HBox.margin>
                                </Button>
                                <Button mnemonicParsing="false" onAction="#handleNewPlayer" prefHeight="39.0" prefWidth="99.0" styleClass="font" text="NEW">
                                    <HBox.margin>
                                        <Insets left="20.0" top="10.0" />
                                    </HBox.margin>
                                </Button>
                                <Button mnemonicParsing="false" onAction="#handleDeletePerson" prefHeight="39.0" prefWidth="99.0" styleClass="font" text="DELETE">
                                    <HBox.margin>
                                        <Insets left="20.0" top="10.0" />
                                    </HBox.margin>
                                </Button>
                            </children>
                        </HBox>
                    </children>
                </AnchorPane>
            </items>
        </SplitPane>
    </children>
</AnchorPane>
