<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="700.0" prefWidth="400.0" styleClass="background" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="OOP_PacMan.controller.UserController">
    <children>
        <SplitPane dividerPositions="0.8445" orientation="VERTICAL" prefHeight="600.0" prefWidth="400.0" styleClass="background">
            <items>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                    <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" layoutY="33.0" prefHeight="53.0" prefWidth="400.0" styleClass="font" text="HIGH SCORES" textFill="WHITE">
                            <font>
                                <Font size="27.0" />
                            </font>
                        </Label>
                        <TableView fx:id="itemtableview" layoutY="110.0" prefHeight="388.0" prefWidth="400.0" styleClass="table-view">
                            <columns>
                                <TableColumn fx:id="nameColumn" prefWidth="200.0" text="Name" />
                                <TableColumn fx:id="scoreColumn" prefWidth="200.0" text="Score" />
                            </columns>
                        </TableView>
                    </children>
                </AnchorPane>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="185.0" prefWidth="400.0">
                    <children>
                        <HBox alignment="CENTER" prefHeight="89.0" prefWidth="400.0">
                            <children>
                                <Button mnemonicParsing="false" onAction="#backtoMain" prefHeight="39.0" prefWidth="99.0" text="BACK">
                                    <HBox.margin>
                                        <Insets top="10.0" />
                                    </HBox.margin>
                           <styleClass>
                              <String fx:value="font" />
                              <String fx:value="title-button" />
                           </styleClass>
                                </Button>
                                <Button mnemonicParsing="false" onAction="#handleDeletePerson" prefHeight="39.0" prefWidth="99.0" text="DELETE">
                                    <HBox.margin>
                                        <Insets left="20.0" top="10.0" />
                                    </HBox.margin>
                           <styleClass>
                              <String fx:value="font" />
                              <String fx:value="title-button" />
                           </styleClass>
                                </Button>
                            </children>
                        </HBox>
                    </children>
                </AnchorPane>
            </items>
        </SplitPane>
    </children>
</AnchorPane>
